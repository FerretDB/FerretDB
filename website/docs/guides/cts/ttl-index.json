{
  "1-create-ttl-index": {
    "request": {
      "createIndexes": "books",
      "indexes": [
        {
          "key": {
            "reservation.date": {
              "$numberInt": "1"
            }
          },
          "name": "reservation_ttl",
          "expireAfterSeconds": {
            "$numberInt": "60"
          }
        }
      ],
      "$db": "{{.Database}}"
    },
    "response": {
      "numIndexesBefore": {
        "$numberInt": "1"
      },
      "numIndexesAfter": {
        "$numberInt": "2"
      },
      "createdCollectionAutomatically": true,
      "ok": {
        "$numberDouble": "1.0"
      }
    }
  },
  "2-insert-ttl-data": {
    "request": {
      "insert": "books",
      "documents": [
        {
          "title": "The Great Gatsby",
          "author": "F. Scott Fitzgerald",
          "reservation": {
            "user": "Ethan Smith",
            "date": {
              "$date": {
                "$numberLong": "1742036400000"
              }
            }
          }
        }
      ],
      "$db": "{{.Database}}"
    },
    "response": {
      "n": {
        "$numberInt": "1"
      },
      "ok": {
        "$numberDouble": "1.0"
      }
    }
  }
}

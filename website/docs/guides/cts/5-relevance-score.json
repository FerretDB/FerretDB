{
  "relevance-score": {
    "request": {
      "find": "books",
      "filter": {
        "$text": {
          "$search": "hunt whales"
        }
      },
      "projection": {
        "title": {
          "$numberInt": "1"
        },
        "author": {
          "$numberInt": "1"
        },
        "summary": {
          "$numberInt": "1"
        },
        "score": {
          "$meta": "textScore"
        }
      },
      "sort": {
        "score": {
          "$meta": "textScore"
        }
      }
    },
    "response": {
      "cursor": {
        "id": {
          "$numberLong": "0"
        },
        "    ns": "new.books",
        "    firstBatch": [
          {
            "        _id": "moby_dick_1851",
            "        title": "Moby Dick",
            "        author": "Herman Melville",
            "        summary": "The narrative follows Ishmael and his voyage aboard the whaling ship Pequod, commanded by Captain Ahab, who is obsessed with hunting the elusive white whale, Moby Dick.",
            "        score": {
              "$numberInt": "3"
            }
          }
        ]
      },
      "  ok": {
        "$numberInt": "1"
      }
    }
  }
}

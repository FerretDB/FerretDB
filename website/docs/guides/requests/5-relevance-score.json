{
  "relevance-score": {
    "request": {
      "find": "books",
      "filter": {
        "$text": {
          "$search": "hunt whales"
        }
      },
      "projection": {
        "title": {
          "$numberInt": "1"
        },
        "author": {
          "$numberInt": "1"
        },
        "summary": {
          "$numberInt": "1"
        },
        "score": {
          "$meta": "textScore"
        }
      },
      "sort": {
        "score": {
          "$meta": "textScore"
        }
      }
    },
    "response": {}
  }
}
